<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorAccent"
    tools:context="wcm.jcrogar.fca.wcm.MainActivity"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="81dp">

    <ImageButton
        android:id="@+id/btnSecurity"
        android:layout_width="162dp"
        android:layout_height="70dp"
        android:layout_marginLeft="16dp"
        android:adjustViewBounds="false"
        android:background="@drawable/button_file"
        android:cropToPadding="false"
        android:scaleType="centerInside"
        app:layout_constraintLeft_toLeftOf="parent"
        app:srcCompat="@drawable/safety"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/btnWcm"
        android:layout_marginStart="16dp" />

    <ImageButton
        android:id="@+id/btnCost"
        android:layout_width="162dp"
        android:layout_height="70dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:background="@drawable/button_file"
        android:scaleType="centerInside"
        app:layout_constraintHorizontal_bias="0.785"
        app:layout_constraintLeft_toRightOf="@+id/btnSecurity"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@drawable/cost"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/btnWcm"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <ImageButton
        android:id="@+id/btnFocused"
        android:layout_width="162dp"
        android:layout_height="70dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/button_file"
        android:scaleType="centerInside"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnSecurity"
        app:srcCompat="@drawable/focus"
        android:layout_marginStart="16dp" />

    <ImageButton
        android:id="@+id/btnAM"
        android:layout_width="162dp"
        android:layout_height="70dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/button_file"
        android:scaleType="centerInside"
        app:layout_constraintHorizontal_bias="0.777"
        app:layout_constraintLeft_toRightOf="@+id/btnFocused"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnCost"
        app:srcCompat="@drawable/auto2"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <ImageButton
        android:id="@+id/btnProM"
        android:layout_width="162dp"
        android:layout_height="70dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/button_file"
        android:scaleType="centerInside"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnFocused"
        app:srcCompat="@drawable/prof"
        android:layout_marginStart="16dp" />

    <ImageButton
        android:id="@+id/btnQuality"
        android:layout_width="162dp"
        android:layout_height="70dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/button_file"
        android:scaleType="centerInside"
        app:layout_constraintHorizontal_bias="0.722"
        app:layout_constraintLeft_toRightOf="@+id/btnProM"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnAM"
        app:srcCompat="@drawable/qual"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <ImageButton
        android:id="@+id/btnLogistic"
        android:layout_width="162dp"
        android:layout_height="70dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/button_file"
        android:scaleType="centerInside"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnProM"
        app:srcCompat="@drawable/log"
        android:layout_marginStart="16dp" />

    <ImageButton
        android:id="@+id/btnEnviroment"
        android:layout_width="162dp"
        android:layout_height="70dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/button_file"
        android:scaleType="centerInside"
        app:layout_constraintHorizontal_bias="0.714"
        app:layout_constraintLeft_toRightOf="@+id/btnLogistic"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnQuality"
        app:srcCompat="@drawable/environmental2"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <ImageButton
        android:id="@+id/btnEquipment"
        android:layout_width="162dp"
        android:layout_height="70dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/button_file"
        android:scaleType="centerInside"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnLogistic"
        app:srcCompat="@drawable/equip"
        android:layout_marginStart="16dp"
        />

    <ImageButton
        android:id="@+id/btnPeople"
        android:layout_width="162dp"
        android:layout_height="70dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/button_file"
        android:scaleType="centerInside"
        app:layout_constraintHorizontal_bias="0.714"
        app:layout_constraintLeft_toRightOf="@+id/btnEquipment"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnEnviroment"
        app:srcCompat="@drawable/peopl"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <ImageButton
        android:id="@+id/btnWcm"
        android:layout_width="0dp"
        android:layout_height="115dp"
        android:background="@android:color/background_dark"
        android:scaleType="fitCenter"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_baja2" />

    <TextView
        android:id="@+id/textView160"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        android:layout_marginTop="29dp"
        android:text="Developed by Paint Shop TAP"
        android:textColor="@android:color/white"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnPeople" />

</android.support.constraint.ConstraintLayout>
    

